    "flagd-demo": "nx run app:flagd-demo",
    "ui": "nx run ui:serve",
    "temp": "nx build",
    "build": "nx build --progress=true --all && nx build ui && cp -R dist/packages/ui/* dist/packages/app/assets/public/ && cp -R scripts/ dist/scripts",
